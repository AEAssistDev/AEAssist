<Window x:Class="AETriggers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="AE轴导出器" Height="180" Width="225">
    <StackPanel Margin="5">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Margin="5" Name="Load" Content="加载Excel表" Click="Load_OnClick" />
            <Button Margin="5" Name="Export" Content="导出" Click="Export_OnClick" />
            <!-- <Button Name="LoadJson" Content="加载导出文件(检验用)" Click="LoadJson_OnClick"></Button> -->
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="作者:" />
            <Label Name="AuthorName" Content="" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="版本:" />
            <Label Name="Version" Content="0" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="目标副本:" />
            <Label Name="TargetDuty" Content="" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label Content="目标职业:" />
            <Label Name="TargetJob" Content="" />
        </StackPanel>

    </StackPanel>
</Window>